A,CAAA,GAAA,CAAA,IAAA;AAEA,YAAA,GAAA,CAAA,IAAA,CAAA,MAAA,KAAA,GAAA,EACA,OAAA,KAAA,QAAA,CAAA,KAAA,CAAA,KAAA,CAAA;AACA,aAAA,QAAA,CAAA,OAAA,CAAA,OAAA;AACA,aAAA,cAAA;AACA,OAbA;AAcA,KAjBA;;AAkBA,IAAA,aAAA,CAAA,SAAA,EAAA;AACA,MAAA,OAAA,CACA,IADA,CACA,qBADA,EACA;AACA,QAAA,IAAA,EAAA;AADA,OADA,EAIA,IAJA,CAIA,GAAA,IAAA;AACA,YAAA,GAAA,CAAA,IAAA,CAAA,MAAA,KAAA,GAAA,EACA,OAAA,KAAA,QAAA,CAAA,KAAA,CAAA,SAAA,CAAA;AACA,aAAA,QAAA,CAAA,OAAA,CAAA,WAAA;AACA,aAAA,cAAA;AACA,OATA;AAUA,KA7BA;;AA8BA;AACA,IAAA,gBAAA,CAAA,IAAA,EAAA;AACA,WAAA,IAAA,GAAA,IAAA;AACA,WAAA,cAAA;AACA,KAlCA;;AAmCA;AACA,IAAA,mBAAA,CAAA,GAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,WAAA,UAAA,GAAA,GAAA;AACA,WAAA,cAAA;AACA,KAxCA;;AAyCA;AACA,IAAA,cAAA,GAAA;AACA,MAAA,OAAA,CACA,GADA,CACA,+BADA,EACA;AACA,QAAA,MAAA,EAAA;AACA,UAAA,UAAA,EAAA,KAAA,UADA;AAEA,UAAA,IAAA,EAAA,KAAA,IAFA;AAGA,UAAA,KAAA,EAAA,KAAA,KAHA;AAIA,UAAA,QAAA,EAAA,KAAA,OAJA;AAKA,UAAA,WAAA,EAAA,cAAA,CAAA,OAAA,CAAA,aAAA;AALA;AADA,OADA,EAUA,IAVA,CAUA,GAAA,IAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA,IAAA;AACA,YAAA,GAAA,CAAA,IAAA,CAAA,MAAA,KAAA,GAAA,EACA,OAAA,KAAA,QAAA,CAAA,KAAA,CAAA,QAAA,CAAA;AACA,aAAA,UAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,OAAA;AACA,aAAA,KAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,KAAA;AACA,OAhBA;AAiBA;;AA5DA,GAZA;;AA0EA,EAAA,OAAA,GAAA;AACA,SAAA,cAAA;AACA;;AA5EA,CAAA","sourcesContent":["<template>\n    <div>\n        <!-- 面包屑 -->\n        <el-breadcrumb separator-class=\"el-icon-arrow-right\">\n            <el-breadcrumb-item :to=\"{ path: '/sectionIndex' }\">返回科室</el-breadcrumb-item>\n            <el-breadcrumb-item>{{ section }}</el-breadcrumb-item>\n        </el-breadcrumb>\n        <el-input v-model=\"query\" placeholder=\"请输入姓名查询\" class=\"doctorInput\">\n            <el-button slot=\"append\" icon=\"el-icon-search\" @click=\"requestDoctors\"></el-button>\n        </el-input>\n        <el-table :data=\"doctorData\" border>\n            <el-table-column label=\"账号\" prop=\"dId\" v-model=\"doctorData.dId\"></el-table-column>\n            <el-table-column label=\"姓名\" prop=\"dName\" v-model=\"doctorData.dName\"></el-table-column>\n            <el-table-column label=\"性别\" prop=\"dGender\" v-model=\"doctorData.dGender\"></el-table-column>\n            <el-table-column label=\"职位\" prop=\"dPost\" v-model=\"doctorData.dPost\"></el-table-column>\n            <el-table-column label=\"部门\" prop=\"dSection\" v-model=\"doctorData.dSection\"></el-table-column>\n            <el-table-column label=\"操作\" prop=\"dSection\">\n                <template slot-scope=\"scope\">\n                    <el-button v-if=\"scope.row.arrangeId == null\" type=\"success\" icon=\"iconfont icon-r-paper\"\n                        style=\"font-size: 14px;\" @click=\"arrangeClick(scope.row.dId)\"> 排班</el-button>\n                    <el-button v-if=\"scope.row.arrangeId != null\" type=\"danger\" icon=\"el-icon-delete\"\n                        style=\"font-size: 14px;\" @click=\"deleteArrange(scope.row.arrangeId)\"> 取消排班</el-button>\n                </template>\n            </el-table-column>\n        </el-table>\n        <!-- 分页 -->\n        <el-pagination @size-change=\"handleSizeChange\" @current-change=\"handleCurrentChange\" background\n            layout=\"total, sizes, prev, pager, next, jumper\" :current-page=\"pageNumber\" :page-size=\"size\"\n            :page-sizes=\"[1, 2, 4, 8, 16]\" :total=\"total\">\n        </el-pagination>\n    </div>\n</template>\n<script>\nimport request from \"@/utils/request.js\";\nexport default {\n    name: \"ArrangeDoctor\",\n    data() {\n        return {\n            section: this.$route.query.section,\n            doctorData: [],\n            total: 3,\n            pageNumber: 1,\n            size: 8,\n            query: \"\",\n        };\n    },\n    methods: {\n        //排班点击\n        arrangeClick(dId) {\n            request\n                .post(\"admin/addArrange\", {\n                        arId: dId + sessionStorage.getItem(\"arrangeDate\"),\n                        arTime: sessionStorage.getItem(\"arrangeDate\"),\n                        dId: dId,\n                })\n                .then((res) => {\n                    console.log(res.data);\n                    \n                    if (res.data.status !== 200)\n                        return this.$message.error(\"已排班\");\n                    this.$message.success(\"排班成功！\");\n                    this.requestDoctors();\n                });\n        },\n        deleteArrange(arrangeId) {\n            request\n                .post(\"admin/deleteArrange\", {\n                        arId: arrangeId,\n                })\n                .then((res) => {\n                    if (res.data.status !== 200)\n                        return this.$message.error(\"排班信息不存在\");\n                    this.$message.success(\"删除排班信息成功！\");\n                    this.requestDoctors();\n                });\n        },\n        //页面大小改变时触发\n        handleSizeChange(size) {\n            this.size = size;\n            this.requestDoctors();\n        },\n        //   页码改变时触发\n        handleCurrentChange(num) {\n            console.log(num);\n            this.pageNumber = num;\n            this.requestDoctors();\n        },\n        //根据部门请求医生信息\n        requestDoctors() {\n            request\n                .get(\"admin/findDoctorBySectionPage\", {\n                    params: {\n                        pageNumber: this.pageNumber,\n                        size: this.size,\n                        query: this.query,\n                        dSection: this.section,\n                        arrangeDate: sessionStorage.getItem(\"arrangeDate\"),\n                    },\n                })\n                .then((res) => {\n                    console.log(res.data);\n                    if (res.data.status !== 200)\n                        return this.$message.error(\"数据请求失败\");\n                    this.doctorData = res.data.data.doctors;\n                    this.total = res.data.data.total;\n                });\n        },\n    },\n    created() {\n        this.requestDoctors();\n    },\n};\n</script>\n<style scope lang=\"scss\">\n.el-breadcrumb {\n    margin-bottom: 10px;\n}\n\n.doctorInput {\n    width: 30%;\n    margin-bottom: 10px;\n}\n</style>"],"sourceRoot":"src/views"}]}