<td><el-input disabled v-model=\"patientData.pPhone\"></el-input></td>\n                </tr>\n                <tr>\n                    <td>身份证号：</td>\n                    <td><el-input disabled v-model=\"patientData.pCard\"></el-input></td>\n                </tr>\n                <tr>\n                    <td>邮箱：</td>\n                    <td><el-input disabled v-model=\"patientData.pEmail\"></el-input></td>\n                </tr>\n                <tr>\n                    <td>出生日期：</td>\n                    <td><el-input disabled v-model=\"patientData.pBirthday\"></el-input></td>\n                </tr>\n                <tr>\n                    <td>年龄：</td>\n                    <td><el-input disabled v-model=\"patientData.pAge\"></el-input></td>\n                </tr>\n\n            </table>\n        </el-card>\n    </div>\n</template>\n<script>\nimport jwtDecode from \"jwt-decode\";\nimport { getToken } from \"@/utils/storage.js\";\nimport request from \"@/utils/request.js\";\nexport default {\n    name: \"PatientCard\",\n    data() {\n        return {\n            userId: \"\",\n            patientData: {}\n        }\n    },\n    methods: {\n        //请求患者信息\n        requestPatient() {\n            request.get(\"patient/findPatientById\", {\n                params: {\n                    pId: this.userId\n                }\n            })\n                .then(res => {\n                    if (res.data.status != 200)\n                        return this.$message.error(\"获取数据失败\");\n                    this.patientData = res.data.data;\n                    console.log(this.patientData);\n                    \n                })\n        }\n    },\n    created() {\n        //解码token信息\n        const token = getToken();\n        this.userId = jwtDecode(token).user_id;\n        this.requestPatient();\n        console.log(this.userId)\n    }\n}\n</script>\n<style lang=\"scss\" scope>\ntd,\nth {\n    white-space: nowrap;\n    padding: 10px;\n}\n</style>"]}]}